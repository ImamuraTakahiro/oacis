= render partial: "shared/breadcrumb", locals: {links: breadcrumb_links_for(@analysis_run)}

.page-header
  %h1 AnalysisRun

%table.table.table-condensed
  %thead
  %tbody
    %tr
      %th ID
      %td= @analysis_run.to_param
    %tr
      %th status
      %td= @analysis_run.status
    %tr
      %th Directory
      %td= @analysis_run.dir

- unless @analysis_run.parameters.empty?
  = render partial: "shared/parameters_table", locals: {parameters_hash: @analysis_run.parameters}

= render partial: "shared/results", locals: {result: @analysis_run.result, result_paths: @analysis_run.result_paths}
